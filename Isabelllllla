df1 = data.frame(date = c(rep('2019-01-01',length(data1$NDVI)),rep('2020-01-01',length(data13$NDVI)),rep('2021-01-01',length(data25$NDVI)),
                          rep('2019-02-01',length(data2$NDVI)),rep('2020-02-01',length(data14$NDVI)),rep('2021-02-01',length(data26$NDVI)),
                          rep('2019-03-01',length(data3$NDVI)),rep('2020-03-01',length(data15$NDVI)),rep('2021-03-01',length(data27$NDVI)),
                          rep('2019-04-01',length(data4$NDVI)),rep('2020-04-01',length(data16$NDVI)),rep('2021-04-01',length(data28$NDVI)),
                          rep('2019-05-01',length(data5$NDVI)),rep('2020-05-01',length(data17$NDVI)),rep('2021-05-01',length(data29$NDVI)),
                          rep('2019-06-01',length(data6$NDVI)),rep('2020-06-01',length(data18$NDVI)),rep('2021-06-01',length(data30$NDVI)),
                          rep('2019-07-01',length(data7$NDVI)),rep('2020-07-01',length(data19$NDVI)),rep('2021-07-01',length(data31$NDVI)),
                          rep('2019-08-01',length(data8$NDVI)),rep('2020-08-01',length(data20$NDVI)),rep('2021-08-01',length(data32$NDVI)),
                          rep('2019-09-01',length(data9$NDVI)),rep('2020-09-01',length(data21$NDVI)),rep('2021-09-01',length(data33$NDVI)),
                          rep('2019-10-01',length(data10$NDVI)),rep('2020-10-01',length(data22$NDVI)),rep('2021-10-01',length(data34$NDVI)),
                          rep('2019-11-01',length(data11$NDVI)),rep('2020-11-01',length(data23$NDVI)),rep('2021-11-01',length(data35$NDVI)),
                          rep('2019-12-01',length(data12$NDVI)),rep('2020-12-01',length(data24$NDVI)),rep('2021-12-01',length(data36$NDVI))),
                 NDVI = c(data1$NDVI,data13$NDVI,data25$NDVI,
                          data2$NDVI,data14$NDVI,data26$NDVI,
                          data3$NDVI,data15$NDVI,data27$NDVI,
                          data4$NDVI,data16$NDVI,data28$NDVI,
                          data5$NDVI,data17$NDVI,data29$NDVI,
                          data6$NDVI,data18$NDVI,data30$NDVI,
                          data7$NDVI,data19$NDVI,data31$NDVI,
                          data8$NDVI,data20$NDVI,data32$NDVI,
                          data9$NDVI,data21$NDVI,data33$NDVI,
                          data10$NDVI,data22$NDVI,data34$NDVI,
                          data11$NDVI,data23$NDVI,data35$NDVI,
                          data12$NDVI,data24$NDVI,data36$NDVI))
names(df1) = c('date','NDVI')
df1 = na.omit(df1)
df1$date = as.Date(df1$date)

library(lubridate)

plot(df1$date,df1$NDVI,type = 'p',xlab = 'year',ylab = 'NDVI',main = 'Scatter Diagram of Pasture Data',yaxt = 'n',col = 'green')
points(subset(df1,year(date)==2020)$date,subset(df1,year(date)==2020)$NDVI,col = 'red')
points(subset(df1,year(date)==2021)$date,subset(df1,year(date)==2021)$NDVI,col = 'blue')
axis(1,1:12)
axis(2,seq(-1,1,0.1),las = 1)




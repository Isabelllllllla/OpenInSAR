data5 = read.csv('201905.csv')
data6 = read.csv('201906.csv')
data7 = read.csv('201907.csv')
data8 = read.csv('201908.csv')
data9 = read.csv('201909.csv')
data10 = read.csv('201910.csv')
data11 = read.csv('201911.csv')
data12 = read.csv('201912.csv')

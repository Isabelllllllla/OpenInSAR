df2 = data.frame(month = c(rep(1,(length(data1$GNDVI)+length(data13$GNDVI)+length(data25$GNDVI))),
                           rep(2,(length(data2$GNDVI)+length(data14$GNDVI)+length(data26$GNDVI))),

##1-----------------------------------------------------------------------------------------------
df2019 = data.frame(month = c(rep(1,(length(data1$NDVI))),
                              rep(2,(length(data2$NDVI))),
                              rep(3,(length(data3$NDVI))),
                              rep(4,(length(data4$NDVI))),
                              rep(5,(length(data5$NDVI))),
                              rep(6,(length(data6$NDVI))),
                              rep(7,(length(data7$NDVI))),
                              rep(8,(length(data8$NDVI))),
                              rep(9,(length(data9$NDVI))),
                              rep(10,(length(data10$NDVI))),
                              rep(11,(length(data11$NDVI))),
                              rep(12,(length(data12$NDVI)))),
                    NDVI = c(data1$NDVI,data2$NDVI,data3$NDVI,data4$NDVI,
                             data5$NDVI,data6$NDVI,data7$NDVI,data8$NDVI,
                             data9$NDVI,data10$NDVI,data11$NDVI,data12$NDVI))
names(df2019) = c('month','NDVI')
df2020 = data.frame(month = c(rep(1,(length(data13$NDVI))),
                              rep(2,(length(data14$NDVI))),
                              rep(3,(length(data15$NDVI))),
                              rep(4,(length(data16$NDVI))),
                              rep(5,(length(data17$NDVI))),
                              rep(6,(length(data18$NDVI))),
                              rep(7,(length(data19$NDVI))),
                              rep(8,(length(data20$NDVI))),
                              rep(9,(length(data21$NDVI))),
                              rep(10,(length(data22$NDVI))),
                              rep(11,(length(data23$NDVI))),
                              rep(12,(length(data24$NDVI)))),
                    NDVI = c(data13$NDVI,data14$NDVI,data15$NDVI,data16$NDVI,
                             data17$NDVI,data18$NDVI,data19$NDVI,data20$NDVI,
                             data21$NDVI,data22$NDVI,data23$NDVI,data24$NDVI))
names(df2020) = c('month','NDVI')
df2021 = data.frame(month = c(rep(1,(length(data25$NDVI))),
                              rep(2,(length(data26$NDVI))),
                              rep(3,(length(data27$NDVI))),
                              rep(4,(length(data28$NDVI))),
                              rep(5,(length(data29$NDVI))),
                              rep(6,(length(data30$NDVI))),
                              rep(7,(length(data31$NDVI))),
                              rep(8,(length(data32$NDVI))),
                              rep(9,(length(data33$NDVI))),
                              rep(10,(length(data34$NDVI))),
                              rep(11,(length(data35$NDVI))),
                              rep(12,(length(data36$NDVI)))),
                    NDVI = c(data25$NDVI,data26$NDVI,data27$NDVI,data28$NDVI,
                             data29$NDVI,data30$NDVI,data31$NDVI,data32$NDVI,
                             data33$NDVI,data34$NDVI,data35$NDVI,data36$NDVI))
names(df2021) = c('month','NDVI')

df2019 = na.omit(df2019)
df2020 = na.omit(df2020)
df2021 = na.omit(df2021)

nrow(df2019)
nrow(df2020)
nrow(df2021)

dftotal = rbind(df2019,df2020,df2021)
dftotal$NDVI = as.numeric(dftotal$NDVI)
mean1 = aggregate(NDVI ~ month,data = dftotal,mean)
mean1$month2 = (mean1$month)^2
reg = lm(NDVI~month+month2,data = mean1)
mean1$expect = reg$coefficients[1]+reg$coefficients[2]*mean1$month+reg$coefficients[3]*mean1$month2


plot(df2019$month,df2019$NDVI,type = 'p',ylim = c(-1,1),xlab = 'month',ylab = 'NDVI',main = 'Scatter Diagram of Corn Data',yaxt = 'n',col = 'green')
points(df2020$month,df2020$NDVI,col = 'red')
points(df2021$month,df2021$NDVI,col = 'blue')
lines(mean1$month,mean1$NDVI,col = 'black')
lines(mean1$month,mean1$expect,col = 'orange')
axis(1,1:12)
axis(2,seq(-1,1,0.1),las = 1)
text(8,0.9,'y = -0.289 + 0.175x - 0.012x^2')
legend('bottomleft',legend = c('2019','2020','2021','mean','quadratic fit'),
       col = c("green","red",'blue','black','orange'),lty = 1,cex = 0.8)


meanG$month2 = (meanG$month)^2
regG = lm(GNDVI~month+month2,data = meanG)
meanG$expect = regG$coefficients[1]+regG$coefficients[2]*meanG$month+regG$coefficients[3]*meanG$month2


plot(df2019G$month,df2019G$GNDVI,type = 'p',ylim = c(-1,1),xlab = 'month',ylab = 'GNDVI',main = 'Scatter Diagram of Corn Data',yaxt = 'n',col = 'green')
